@model IList<Location>
@{
    ViewData["Title"] = "People";
}

<h1>@ViewData["Title"]</h1>

<a class="btn btn-primary" asp-action="Create">New Person</a>

<table class="table table-striped table-dark">
    <thead>
    <tr>
        <th>Name</th>
        <th>Adress</th>
        <th>Description</th>
        <th>Action</th>
    </tr>
    </thead>
    <tbody>
    @foreach (var person in Model)
    {
        <partial name="RowPartial" model="person" />
    }
    </tbody>
</table>